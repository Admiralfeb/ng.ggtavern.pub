{
  "name": "ggtavern",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npm run generate-key-file",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --prod --aot",
    "test": "ng test --code-coverage",
    "test-chrome-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless --code-coverage",
    "test-firefox-ci": "ng test --no-watch --no-progress --browsers=FirefoxHeadless --code-coverage",
    "report-coverage": "codecov -f coverage/*.json",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-chrome-ci": "ng e2e --port 0 --protractor-config=e2e/protractor-chrome-ci.conf.js",
    "e2e-firefox-ci": "ng e2e --port 0 --protractor-config=e2e/protractor-firefox-ci.conf.js",
    "generate-key-file": "ts-node scripts/generateKeyFile.ts",
    "deploy": "firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.2",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.2.2",
    "@angular/compiler": "~8.2.2",
    "@angular/core": "~8.2.2",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.2.2",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "~8.2.2",
    "@angular/platform-browser-dynamic": "~8.2.2",
    "@angular/router": "~8.2.2",
    "firebase": "^6.6.2",
    "hammerjs": "^2.0.8",
    "moment-timezone": "^0.5.26",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.5",
    "@angular/cli": "~8.3.5",
    "@angular/compiler-cli": "~8.2.2",
    "@angular/language-service": "~8.2.2",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "~12.7.5",
    "codecov": "^3.6.1",
    "codelyzer": "^5.1.1",
    "dotenv": "^8.1.0",
    "firebase-tools": "^7.3.2",
    "jasmine-core": "~3.4.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
