{
  "name": "ggtavern",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npm run generate-key-file",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --prod --aot",
    "test": "ng test --code-coverage",
    "test-chrome-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless --code-coverage",
    "test-firefox-ci": "ng test --no-watch --no-progress --browsers=FirefoxHeadless --code-coverage",
    "report-coverage": "codecov -f coverage/*.json",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-chrome-ci": "ng e2e --protractor-config=e2e/protractor-chrome-ci.conf.js",
    "e2e-firefox-ci": "ng e2e --protractor-config=e2e/protractor-firefox-ci.conf.js",
    "generate-key-file": "ts-node scripts/generateKeyFile.ts",
    "deploy": "ng run gg-tavern:deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.13",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.13",
    "@angular/compiler": "8.2.13",
    "@angular/core": "8.2.13",
    "@angular/fire": "5.2.1",
    "@angular/forms": "8.2.13",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/router": "8.2.13",
    "firebase": "7.2.3",
    "hammerjs": "2.0.8",
    "moment-timezone": "0.5.27",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.18",
    "@angular/cli": "8.3.18",
    "@angular/compiler-cli": "8.2.13",
    "@angular/language-service": "8.2.13",
    "@types/jasmine": "3.4.6",
    "@types/jasminewd2": "2.0.8",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "12.12.6",
    "codecov": "3.6.1",
    "codelyzer": "5.2.0",
    "dotenv": "8.2.0",
    "firebase-tools": "7.6.2",
    "jasmine-core": "3.5.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.1",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-firefox-launcher": "1.2.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-junit-reporter": "2.0.1",
    "protractor": "5.4.2",
    "ts-node": "8.4.1",
    "tslint": "5.20.1",
    "typescript": "3.5.3",
    "@angular-devkit/architect": "0.803.18",
    "fuzzy": "0.1.3",
    "inquirer": "7.0.0",
    "inquirer-autocomplete-prompt": "1.0.1"
  }
}
